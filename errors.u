get_int : '{IO, Exception} Int
get_int = do
  line = !readLine
  Optional.getOrBug "Invalid Input" (Int.fromText line)

main : '{IO, Exception} ()
main = do
  n = get_int()
  m = n/3
  printLine (Int.toText (m))

